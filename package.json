{
  "name": "cartridge-sass-legacy",
  "version": "0.3.0",
  "description": "DEPRECATED: Windows friendly Sass expansion pack for cartridge",
  "keywords": [
    "gulp",
    "dependency",
    "cartridge",
    "sass",
    "libsass",
    "postcss"
  ],
  "homepage": "https://github.com/cartridge/cartridge-sass-legacy",
  "bugs": {
    "url": "https://github.com/cartridge/cartridge-sass-legacy/issues"
  },
  "license": "MIT",
  "author": "Code ComputerLove <npm@codecomputerlove.co.uk>",
  "main": "task.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/cartridge/cartridge-sass-legacy.git"
  },
  "scripts": {
    "postinstall": "node scripts/postinstall.js",
    "preuninstall": "node scripts/preuninstall.js"
  },
  "dependencies": {
    "autoprefixer": "6.3.4",
    "cartridge-module-util": "~0.2.1",
    "css-mqpacker": "4.0.1",
    "cssnano": "3.5.2",
    "gulp-if": "2.0.0",
    "gulp-postcss": "6.1.0",
    "gulp-rename": "1.2.2",
    "gulp-sass": "1.3.3",
    "gulp-sass-generate-contents": "0.1.3",
    "gulp-sourcemaps": "1.6.0",
    "postcss-pxtorem": "3.3.1"
  },
  "devDependencies": {
    "chai": "1.10.0",
    "chai-fs": "^0.1.0",
    "cz-conventional-changelog": "^1.1.5",
    "fs-extra": "^0.26.7",
    "ghooks": "^1.2.1",
    "mocha": "^2.4.5",
    "validate-commit-msg": "^2.6.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100,
      "subjectPattern": ".+",
      "subjectPatternErrorMsg": "subject does not match subject pattern!",
      "helpMessage": ""
    }
  }
}
